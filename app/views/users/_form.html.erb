<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <div class="form-group">
        <%= form_for user do |f| %>
        <%= f.label :image %>
          <%= f.file_field :image %><br>

              <%= f.submit class: 'btn btn-primary btn-block' %>
            </div>
        <% end %>
      </div>
    </div>
  </div>
</div>